{
    "name": "yaroslawww/nova-list-card",
    "description": "Laravel Nova card with resources list",
    "keywords": [
        "laravel",
        "nova",
        "card",
        "listing"
    ],
    "homepage": "https://github.com/yaroslawww/nova-list-card",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^3.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "orchestra/testbench": "^6.23",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-laravel": "^1.5",
        "vimeo/psalm": "^4.16"
    },
    "autoload": {
        "psr-4": {
            "NovaListCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaListCard\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaListCard\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --colors=always --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
